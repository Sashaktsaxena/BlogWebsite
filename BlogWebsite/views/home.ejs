<%- include("partials/header.ejs") %>
<h1>Home</h1>
<p><%= text %></p>
<% posts.forEach(function(post) { %>
  <h1><%= post.title %></h1>
  <p><%= post.post.substring(0, 200) %><a href="/<%=post._id%>">Read me</a></p>
<% }) %>
<div class="compose">
  <form action="/compose" method="get" >
    <button class="com">+</button>
  </form>
</div>
